{
  "name": "@repo/oga-admin",
  "version": "1.0.0",
  "description": "Introspects your models and creates an Admin Page with CRUD Functionality",
  "main": "dist/index.js",
  "scripts": {
    "dev:ts": "tsc --watch",
    "dev:css": "npx tailwindcss -i src/global.css -o dist/styles.css --watch",
    "dev": "concurrently \"npm run dev:ts\" \"npm run dev:css\"",
    "build:ts": "tsc",
    "build:css": "npx tailwindcss -i src/global.css -o dist/styles.css",
    "build": "npm run build:ts && npm run build:css",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "ci": "npm run build && npm run check-format && npm run check-exports",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm"
  },
  "keywords": [
    "NextJS",
    "Prisma",
    "Mongoose",
    "Cloudinary",
    "TypeScript",
    "TailwindCSS",
    "Admin",
    "Admin Library"
  ],
  "author": "Daniel Azubuine",
  "license": "ISC",
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.447.0",
    "react-dom": "^18.3.1",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "next": "^14.0.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@changesets/cli": "^2.27.8",
    "@repo/typescript-config": "*",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2"
  }
}
